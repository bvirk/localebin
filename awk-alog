#! /usr/bin/awk -f
# Filtered view of lines and timestamp where items 2-4 is removed
# based on field seperator being ']' - args to this script being -F ] -v withref=no|yes
{ 
    time=substr($1,13,8);
    mes=gensub(/\\n/,"\n","g",$5);
    if (withref == "no") {
        print time,gensub(/referer.+$/,"","g",mes);
    } else {
        print time,mes;
    }
}       

