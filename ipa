#!/opt/jdk1.8.0_202/bin/java bsh.Interpreter
final String __FILENAME__ =System.getProperty("sun.java.command").replaceAll("^.+?\\s","").replaceAll("\\s.+$","");
addClassPath(dirname(__FILENAME__)); 
importCommands("/beanshell");
globals();

void syntax() {
	print("ipa function parameter");
}
void what() {
	p(
		 "functions is one off\n"
		+"edit pages[/page]\n"
		);
}
if (bsh.args.length < 2) {
	syntax();
	what();
	return;
}
String parm1 =bsh.args[1];
List args = new ArrayList();
for (int i=1; i< bsh.args.length; i++)
	args.add(bsh.args[i]);
switch (bsh.args[0]) {
case "edit":
	Runtime.getRuntime().exec(new String[]{"/bin/bash","-c","uaed "+editPage(parm1)});
	break;
case "cmd":
	List l = curlCommand(args);
	p(String.join("\n",l));
	//p("command " + parm1);
	break;
default:
	p(bsh.args[0]+" what?");
}

